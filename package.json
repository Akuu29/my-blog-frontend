{
  "name": "my-blog-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:stg1": "tsc && vite build --mode stg",
    "build:prd2": "tsc && vite build --mode prd",
    "build:stg": "tsc && CLOUDFLARE_ENV=stg vite build --mode stg",
    "build:prd": "tsc && CLOUDFLARE_ENV=prd vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview:stg": "npm run build:stg && wrangler dev",
    "preview:prd": "npm run build:prd && wrangler dev",
    "deploy-dry-run:stg": "npm run build:stg && wrangler deploy --dry-run",
    "deploy-dry-run:prd": "npm run build:prd && wrangler deploy --dry-run",
    "delete-dry-run:stg": "wrangler delete --env stg --dry-run",
    "delete-dry-run:prd": "wrangler delete --env prd --dry-run"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.16",
    "@mui/material": "^5.15.16",
    "@uiw/react-markdown-preview": "^5.1.3",
    "@uiw/react-md-editor": "^4.0.4",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "firebase": "^11.1.0",
    "qs": "^6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "rehype-sanitize": "^6.0.0",
    "ts-pattern": "^5.5.0"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.8.0",
    "@types/qs": "^6.9.18",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^6.3.6",
    "wrangler": "^4.22.0"
  }
}
